{
  "expo": {
    "name": "Recipes App",
    "slug": "Recipes-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./Data/images/icon.png",
    "splash": {
      "image": "./Data/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#eff7e1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Data/images/adaptive-icon.png",
        "backgroundColor": "#96fbc4"
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "react-router-native"
          ]
        }
      }
    }
  }
}
