{
  "expo": {
    "name": "Recipes App",
    "slug": "Recipes-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./Data/images/tray.png",
    "splash": {
      "image": "./Data/images/tray.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Data/images/tray.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "react-router-native"
          ]
        }
      }
    }
  }
}
